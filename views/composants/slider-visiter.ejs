<div class="slider-containerVisiter">
  <h3 style="text-align: start; margin-left: 10px;">Articles visiter</h3>
  <div class="sliderVisiter">
    <% visiter.forEach((produit) => { %>
    <div class="article-carte">
      <img id="url" src="<%= produit.url %>" alt="<%= produit.nom %>">
      <br>
      <p style="text-align: center; text-transform: uppercase;" id="nom"> <strong> <%= produit.nom %> </strong> </p>
      <span id="info"><%= produit.info %></span>
      <div class="prix">
        <span class="prix-actuel" style="color: #293847; font-weight: bolder; font-size: 1.2rem;" id="prix"><%= produit.prix %> CFA</span>
      </div>

      <button class="add-to-cart-btn" data-product-id="<%= produit.id %>" data-product-nom="<%= produit.nom %>" data-product-prix="<%= produit.prix %>" data-product-url="<%= produit.url %>" data-product-info="<%= produit.info %>" onclick="addToCart(this)">Ajouter</button>

    </div>
    <% }); %>
    <div>

    </div>
  </div>